<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>spring-cloud-zookeeper</groupId>
    <artifactId>spring-cloud-zookeeper-register</artifactId>
    <version>1.0-SNAPSHOT</version>

    <packaging>pom</packaging>
	
    <modules>
        <module>spring-cloud-zookeeper-client</module>
        <module>spring-cloud-zookeeper-server</module>
        <module>spring-cloud-zookeeper-server2</module>
    </modules>


    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>